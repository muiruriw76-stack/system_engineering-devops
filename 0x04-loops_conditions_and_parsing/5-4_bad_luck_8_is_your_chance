 #!/usr/bin/env bash
Displaying numbers and special bad luck messages for 4, 9, 17
num=1
while [ $num -le 20 ]
do
    echo "$num"
    case $num in
        4)
            echo "bad luck from China"
            ;;
        9)
            echo "bad luck from Japan"
            ;;
        17)
            echo "bad luck from Italy"
            ;;
    esac
    num=$((num + 1))
done
